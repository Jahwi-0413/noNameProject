                <Button fx:id="mainAddBtn" layoutX="57.0" layoutY="80.0" mnemonicParsing="false" onAction="#clickAdd" prefHeight="40.0" style="-fx-background-color: #83dcb7; -fx-border-color: white;" text="추가" textFill="WHITE">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
                <Button fx:id="mainDeleteBtn" layoutX="160.0" layoutY="80.0" mnemonicParsing="false" onAction="#clickDelete" prefHeight="40.0" style="-fx-background-color: #83dcb7; -fx-border-color: white;" text="삭제" textFill="WHITE">
                    <font>
                        <Font size="20.0" />
                    </font>
                </Button>
            </children>
        </Pane>
    </children>
</AnchorPane>
