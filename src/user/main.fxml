<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="850.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Pane prefHeight="70.0" prefWidth="850.0" style="-fx-background-color: skyblue;">
            <children>
                <Button fx:id="tourBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickTour" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: skyblue; -fx-border-color: white;" text="관광지" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font></Button>
                <Button fx:id="restrantBtn" layoutX="119.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickRestrant" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: skyblue; -fx-border-color: white;" text="음식점" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font></Button>
                <Button fx:id="lodgmentBtn" layoutX="222.0" layoutY="14.0" mnemonicParsing="false" onAction="#clickLodg" prefHeight="40.0" prefWidth="90.0" style="-fx-background-color: skyblue; -fx-border-color: white;" text="숙박" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font></Button>
                <TextField fx:id="textField" layoutX="465.0" layoutY="15.0" prefHeight="40.0" prefWidth="295.0" />
                <Button fx:id="searchBtn" layoutX="766.0" layoutY="15.0" mnemonicParsing="false" onAction="#clickSearch" prefHeight="40.0" style="-fx-background-color: skyblue; -fx-border-color: white;" text="검색" textFill="WHITE">
               <font>
                  <Font size="20.0" />
               </font></Button>
            <CheckBox fx:id="gumiCheck" layoutX="324.0" layoutY="25.0" mnemonicParsing="false" onAction="#checkGumi" text="구미">
               <font>
                  <Font size="15.0" />
               </font></CheckBox>
            <CheckBox fx:id="daeguCheck" layoutX="396.0" layoutY="25.0" mnemonicParsing="false" onAction="#checkDaegu" text="대구">
               <font>
                  <Font size="15.0" />
               </font>
            </CheckBox>
            </children>
        </Pane>
        <Pane fx:id="contentPane" layoutY="70.0" prefHeight="402.0" prefWidth="850.0">
            <children>
                <TableView layoutX="25.0" layoutY="8.0" prefHeight="387.0" prefWidth="800.0" style="-fx-background-color: white;">
                    <columns>
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C1" />
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C2" />
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C3" />
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C4" />
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C5" />
                        <TableColumn prefWidth="75.0" style="-fx-background-color: skyblue;" text="C6" />
                    </columns>
                </TableView>
            </children>
        </Pane>
    </children>
</AnchorPane>
